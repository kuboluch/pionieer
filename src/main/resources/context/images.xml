<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/context
                        http://www.springframework.org/schema/context/spring-context-2.5.xsd
                        http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="playerLeftImage" class="org.newdawn.slick.Image">
        <constructor-arg value="images/supermanLeft.png"/>
    </bean>
    <bean id="playerRightImage" class="org.newdawn.slick.Image">
        <constructor-arg value="images/supermanRight.png"/>
    </bean>
    <bean id="pickaxeImage" class="org.newdawn.slick.Image">
        <constructor-arg value="images/pickaxe.png"/>
    </bean>
</beans>
